<?xml version="1.0" encoding="UTF-8" ?>
<odoo>
    <record id="view_report_pos_order_search" model="ir.ui.view">
        <field name="name">report.pos.order.search.inherit.psae_pos_salesperson</field>
        <field name="model">report.pos.order</field>
        <field name="inherit_id" ref="point_of_sale.view_report_pos_order_search"/>
        <field name="arch" type="xml">
            <filter name="product_category" position="after">
                <filter string="Salesperson" name="salesperson" context="{'group_by':'salesperson_id'}"/>
            </filter>
        </field>
    </record>
</odoo>
