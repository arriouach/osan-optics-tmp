<?xml version="1.0" encoding="utf-8" ?>
<odoo>
    <template id="redirect_form">
        <!-- Tabby generates a unique URL for each payment request -->
        <form t-att-action="api_url" method="get">
            <input type="hidden" name="sessionId" t-att-value="sessionId" />
            <input type="hidden" name="apiKey" t-att-value="apiKey" />
            <input type="hidden" name="product" t-att-value="product" />
            <input type="hidden" name="merchantCode" t-att-value="merchantCode" />
        </form>
    </template>
</odoo>
